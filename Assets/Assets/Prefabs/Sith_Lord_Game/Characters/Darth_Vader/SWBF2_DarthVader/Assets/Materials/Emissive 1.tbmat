    Color = 0.0703601 0.109462 0.0544803
    Intensity = 100
    UV Set = 0
    Glow = 0 0 0
@End

@Sub SRMerge = SRMerge
    Texture Tiling = 1
    Tile U = 1
    Offset U = 0
